   "source": [
    "from astropy import units as u\n",
    "from astropy.constants import G, M_sun"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "R = 1e4*u.pc\n",
    "sigma = 150*u.km/u.s"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "M = 5*sigma**2*R/G"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "mass = M.to(u.kg)/M_sun"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "mass: 2.615721e+11\n"
     ]
    }
   ],
   "source": [
    "print(\"mass: {:3e}\".format(mass))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
